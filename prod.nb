     RowBox[{"If", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"schema", "==", 
        RowBox[{"Reverse", "[", "schema", "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"Coefficient", "[", 
        RowBox[{
         RowBox[{"D", "[", 
          RowBox[{"S", ",", 
           SubscriptBox["M", "i"]}], "]"}], ",", 
         RowBox[{"GenerateExpression", "[", "schema", "]"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
